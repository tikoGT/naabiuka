{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "anhskohbo/no-captcha": "^3.4.0",
        "barryvdh/laravel-dompdf": "^2.0.0",
        "brick/math": "^0.9.3",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^2.5",
        "laravel/framework": "^10.10",
        "laravel/passport": "^11.0",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.8",
        "maatwebsite/excel": "^3.1",
        "maba/math": "^1.0",
        "nwidart/laravel-modules": "8.2.0",
        "millat/laravel-hooks": "^1.3",
        "phpmailer/phpmailer": "^6.4",
        "spatie/laravel-activitylog": "^4.7.2",
        "stripe/stripe-php": "^7.113",
        "yajra/laravel-datatables": "^10.0",
        "milon/barcode": "^10.0",
        "joshbrw/laravel-module-installer": "^2.0",
        "paypal/paypalhttp": "^1.0"
    },
    "require-dev": {
        "spatie/laravel-ignition": "^2.0",
        "fakerphp/faker": "^1.9.1",
        "kitloong/laravel-migrations-generator": "^6.0",
        "laravel/breeze": "^1.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^7.0",
        "orangehill/iseed": "^3.0",
        "phpunit/phpunit": "^10.1",
        "laravel/pint": "^1.13",
        "pestphp/pest": "^2.33",
        "pestphp/pest-plugin-laravel": "^2.2",
        "pestphp/pest-plugin-faker": "^2.0",
        "laravel/dusk": "^7.12"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": false,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "joshbrw/laravel-module-installer": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Infoamin\\Installer\\": "packages/infoamin/laravel-installer/src/"
        },
        "files": [
            "app/helpers/helpers.php",
            "app/helpers/strings.php",
            "app/helpers/files.php",
            "app/helpers/langdatetime.php",
            "app/helpers/numbers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan app:register-git-hooks"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}